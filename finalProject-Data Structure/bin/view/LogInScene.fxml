<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="pane" prefHeight="600.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.LogInController">
   <children>
      <Label alignment="CENTER" layoutX="93.0" layoutY="14.0" prefHeight="67.0" prefWidth="330.0" text="Sign In">
         <font>
            <Font size="37.0" />
         </font>
      </Label>
      <TextField fx:id="userNameField" layoutX="142.0" layoutY="117.0" prefHeight="42.0" prefWidth="281.0" promptText="Username">
         <font>
            <Font size="23.0" />
         </font>
      </TextField>
      <TextField fx:id="passwordField" layoutX="142.0" layoutY="201.0" prefHeight="50.0" prefWidth="281.0" promptText="Password">
         <font>
            <Font size="22.0" />
         </font>
      </TextField>
      <Button layoutX="213.0" layoutY="283.0" mnemonicParsing="false" onAction="#logIn" prefHeight="35.0" prefWidth="112.0" text="Log In" />
      <Label fx:id="userNameLabel" layoutX="141.0" layoutY="167.0" prefHeight="35.0" prefWidth="288.0" />
      <Label fx:id="passwordLabel" layoutX="139.0" layoutY="251.0" prefHeight="35.0" prefWidth="288.0" />
      <Button layoutX="456.0" layoutY="14.0" mnemonicParsing="false" onAction="#goToHomePage" prefHeight="27.0" prefWidth="104.0" text="Home" />
   </children>
</AnchorPane>
